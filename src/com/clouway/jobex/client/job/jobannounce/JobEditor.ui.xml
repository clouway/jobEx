<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:d='urn:import:com.google.gwt.user.datepicker.client'>

  <g:HTMLPanel>
    <div>
      Company: <g:TextBox ui:field="company" />
    </div>

    <div>
      Position: <g:TextBox ui:field="position" />
    </div>

    <div>
      Category: <g:TextBox ui:field="category" />
    </div>

    <div>
      Location: <g:TextBox ui:field="location" />
    </div>

    <div>
      Expiration Date: <d:DateBox ui:field="date" />
    </div>



  </g:HTMLPanel>
</ui:UiBinder>