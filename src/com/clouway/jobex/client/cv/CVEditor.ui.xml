<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
             xmlns:b2="urn:import:com.github.gwtbootstrap.datepicker.client.ui"
        >

  <ui:style>
    .datePicker {
    width: 100px;
  }

  </ui:style>

  <g:HTMLPanel>


    <g:Label text="name"/>
    <b:TextBox ui:field="name"/>


    <g:Label text="Date of birth"/>
    <b2:DateBox ui:field="datePicker" format="dd/mm/yyyy" autoClose="true"/>


    <g:Label text="Gender"/>
    <b:ListBox ui:field="genderSelectBox">
    </b:ListBox>


    <g:Label text="phoneNumber"/>
    <b:TextBox ui:field="phoneNumber"/>

    <g:Label text="skills"/>
    <b:TextBox ui:field="skills"/>


  </g:HTMLPanel>


</ui:UiBinder>