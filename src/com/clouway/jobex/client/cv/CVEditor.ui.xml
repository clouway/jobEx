<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
             xmlns:b2="urn:import:com.github.gwtbootstrap.datepicker.client.ui"
        >

  <g:HTMLPanel>


    <g:Label text="Name"/>
    <b:TextBox ui:field="name"/>


    <g:Label text="Date of Birth"/>
    <b2:DateBox ui:field="datePicker" format="dd/mm/yyyy" autoClose="true"/>


    <g:Label text="Phone Number"/>
    <b:TextBox ui:field="phoneNumber"/>

    <g:Label text="Skills"/>
    <b:TextBox ui:field="skills"/>


  </g:HTMLPanel>


</ui:UiBinder>