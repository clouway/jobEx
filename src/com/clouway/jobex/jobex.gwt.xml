<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="jobex">

  <!-- Inherit the core Web Toolkit stuff.                  -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name="com.google.gwt.place.Place"/>
  <inherits name="com.google.web.bindery.requestfactory.RequestFactory"/>
  <inherits name="com.google.gwt.activity.Activity"/>

  <!-- Specify the app entry point class.                   -->
  <entry-point class='com.clouway.jobex.client.Jobex'/>
  <!--<servlet path="/ActionDispatcherService" class="com.clouway.jobex.server.communication.ActionDispatcherServiceImpl"/>-->
  <!--<servlet path="/ApplicationSerivce" class="com.clouway.jobex.server.communication.ApplicationServiceImpl"/>-->

  <source path="client"/>
  <source path="shared"/>
</module>